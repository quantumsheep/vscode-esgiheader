{
  "name": "esgiheader",
  "displayName": "ESGI Header",
  "description": "ESGI header for VSCode : www.esgi.fr",
  "version": "1.0.6",
  "publisher": "QuantumSheep",
  "icon": "esgi.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantumSheep/vscode-esgiheader"
  },
  "bugs": {
    "url": "https://github.com/QuantumSheep/vscode-esgiheader/issues"
  },
  "homepage": "https://github.com/QuantumSheep/vscode-esgiheader",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "esgiheader.username": {
          "type": "string",
          "description": "Username to use in ESGI headers"
        },
        "esgiheader.email": {
          "type": "string",
          "description": "Email address to use in ESGI headers"
        }
      }
    },
    "commands": [
      {
        "command": "esgiheader.insert",
        "title": "Insert ESGI header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "esgiheader.insert",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./out/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.10.9",
    "@types/vscode": "^1.43.0",
    "tslint": "^5.20.1",
    "typescript": "^2.9.2",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "moment": "^2.29.4"
  }
}